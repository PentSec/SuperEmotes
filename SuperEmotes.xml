                </Size>
            </Texture>
        </Layer>
        <Layer level="DISABLED">
            <Texture name="TestButton_DisabledTexture" file="Interface\AddOns\SuperEmotes\Emoji\emSmiley.tga">
                <Size>
                    <AbsDimension x="24" y="24"/>
                </Size>
            </Texture>
        </Layer>
    </Layers>
    <Scripts>
        <OnLoad>
            self:RegisterForClicks("LeftButtonDown")
            self:RegisterForDrag("LeftButton")
        </OnLoad>
        <OnClick>
            Lib_ToggleDropDownMenu(1, nil, EmoticonChatFrameDropDown, self, 0, 0)
        </OnClick>
        <OnDragStart>
            if IsControlKeyDown() then
                self:StartMoving()
            end
        </OnDragStart>
        <OnDragStop>
            self:StopMovingOrSizing()
        </OnDragStop>
    </Scripts>
</Button>
